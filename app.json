{
  "expo": {
    "name": "Cast4TV",
    "slug": "Cast4TV",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashArt.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "The app uses the local network to discover Cast-enabled devices on your WiFi network."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anyamborogass.Cast4TV"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "react-native-google-cast",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1caf4d78-6afe-4847-a724-03e7aa877930"
      }
    }
  }
}
